<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.sofproject.alsa.topo.provider"
         name="Alsa Topology Provider"
         point="org.sofproject.topo.providers">
      <factory
            class="org.sofproject.alsa.topo.model.AlsaTopoFactory"
            file_extension="tplg">
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.sofproject.topo.ui.editor.TopoEditor"
            default="false"
            extensions="tplg"
            id="org.sofproject.alsa.topo.editor"
            name="Alsa Topology Editor">
      </editor>
   </extension>

</plugin>
